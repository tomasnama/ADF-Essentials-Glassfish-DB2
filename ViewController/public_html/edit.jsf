                                              label="#{bindings.Phoneno.hints.label}"
                                              required="#{bindings.Phoneno.hints.mandatory}"
                                              columns="#{bindings.Phoneno.hints.displayWidth}"
                                              maximumLength="#{bindings.Phoneno.hints.precision}"
                                              shortDesc="#{bindings.Phoneno.hints.tooltip}" id="it6">
                                    <f:validator binding="#{bindings.Phoneno.validator}"/>
                                </af:inputText>
                                <af:inputDate value="#{bindings.Hiredate.inputValue}"
                                              label="#{bindings.Hiredate.hints.label}"
                                              required="#{bindings.Hiredate.hints.mandatory}"
                                              columns="#{bindings.Hiredate.hints.displayWidth}"
                                              shortDesc="#{bindings.Hiredate.hints.tooltip}" id="id1">
                                    <f:validator binding="#{bindings.Hiredate.validator}"/>
                                    <af:convertDateTime pattern="#{bindings.Hiredate.format}"/>
                                </af:inputDate>
                                <af:inputText value="#{bindings.Job.inputValue}" label="#{bindings.Job.hints.label}"
                                              required="#{bindings.Job.hints.mandatory}"
                                              columns="#{bindings.Job.hints.displayWidth}"
                                              maximumLength="#{bindings.Job.hints.precision}"
                                              shortDesc="#{bindings.Job.hints.tooltip}" id="it7">
                                    <f:validator binding="#{bindings.Job.validator}"/>
                                </af:inputText>
                                <af:inputText value="#{bindings.Edlevel.inputValue}"
                                              label="#{bindings.Edlevel.hints.label}"
                                              required="#{bindings.Edlevel.hints.mandatory}"
                                              columns="#{bindings.Edlevel.hints.displayWidth}"
                                              maximumLength="#{bindings.Edlevel.hints.precision}"
                                              shortDesc="#{bindings.Edlevel.hints.tooltip}" id="it8">
                                    <f:validator binding="#{bindings.Edlevel.validator}"/>
                                    <af:convertNumber groupingUsed="false" pattern="#{bindings.Edlevel.format}"/>
                                </af:inputText>
                                <af:inputText value="#{bindings.Sex.inputValue}" label="#{bindings.Sex.hints.label}"
                                              required="#{bindings.Sex.hints.mandatory}"
