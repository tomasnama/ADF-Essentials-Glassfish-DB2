                            <af:panelFormLayout id="pfl1">
                                <af:inputText value="#{bindings.Empno.inputValue}" label="#{bindings.Empno.hints.label}"
                                              required="#{bindings.Empno.hints.mandatory}"
                                              columns="#{bindings.Empno.hints.displayWidth}"
                                              maximumLength="#{bindings.Empno.hints.precision}"
                                              shortDesc="#{bindings.Empno.hints.tooltip}" id="it1">
                                    <f:validator binding="#{bindings.Empno.validator}"/>
                                </af:inputText>
                                <af:inputText value="#{bindings.Firstnme.inputValue}"
                                              label="#{bindings.Firstnme.hints.label}"
                                              required="#{bindings.Firstnme.hints.mandatory}"
                                              columns="#{bindings.Firstnme.hints.displayWidth}"
                                              maximumLength="#{bindings.Firstnme.hints.precision}"
                                              shortDesc="#{bindings.Firstnme.hints.tooltip}" id="it2">
                                    <f:validator binding="#{bindings.Firstnme.validator}"/>
                                </af:inputText>
                                <af:inputText value="#{bindings.Midinit.inputValue}"
                                              label="#{bindings.Midinit.hints.label}"
                                              required="#{bindings.Midinit.hints.mandatory}"
                                              columns="#{bindings.Midinit.hints.displayWidth}"
                                              maximumLength="#{bindings.Midinit.hints.precision}"
                                              shortDesc="#{bindings.Midinit.hints.tooltip}" id="it3">
                                    <f:validator binding="#{bindings.Midinit.validator}"/>
                                </af:inputText>
                                <af:inputText value="#{bindings.Lastname.inputValue}"
                                              label="#{bindings.Lastname.hints.label}"
                                              required="#{bindings.Lastname.hints.mandatory}"
                                              columns="#{bindings.Lastname.hints.displayWidth}"
                                              maximumLength="#{bindings.Lastname.hints.precision}"
                                              shortDesc="#{bindings.Lastname.hints.tooltip}" id="it4">
                                    <f:validator binding="#{bindings.Lastname.validator}"/>
                                </af:inputText>
                                <af:inputText value="#{bindings.Workdept.inputValue}"
                                              label="#{bindings.Workdept.hints.label}"
                                              required="#{bindings.Workdept.hints.mandatory}"
                                              columns="#{bindings.Workdept.hints.displayWidth}"
                                              maximumLength="#{bindings.Workdept.hints.precision}"
                                              shortDesc="#{bindings.Workdept.hints.tooltip}" id="it5">
                                    <f:validator binding="#{bindings.Workdept.validator}"/>
